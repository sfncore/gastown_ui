# Gas Town UI Environment Variables
# Copy this file to .env and customize for your environment

# WebSocket URL for real-time updates
# Development: ws://localhost:8080/ws
# Production: wss://api.gastown.app/ws
VITE_WS_URL=ws://localhost:8080/ws

# Enable debug logging in production (optional)
# VITE_DEBUG=true

# Enable mock API for development/testing (optional)
# VITE_MOCK_API=true
